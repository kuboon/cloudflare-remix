{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "NO_D1_WARNING=true wrangler pages dev ./public --port 3002 --compatibility-date=2023-03-02b --persist",
    "db:create": "wrangler d1 create myapp",
    "db:setup": "wrangler d1 execute myapp --file ./app/db/setup.sql --local",
    "db:start": "wrangler dev --persist"
  },
  "dependencies": {
    "@hexagon/base64": "^1.1.26",
    "@remix-run/cloudflare": "^1.13.0",
    "@remix-run/cloudflare-pages": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@resvg/resvg-wasm": "^2.4.1",
    "cross-env": "^7.0.3",
    "fido2-lib": "^3.3.5",
    "kysely-d1": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^6.0.0",
    "satori": "^0.4",
    "yoga-wasm-web": "^0.3.2"
  },
  "overrides":{
    "@peculiar/webcrypto": "file:./empty_package"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "better-sqlite3": "^8.0.1",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5",
    "wrangler": "^2.11.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
